<launch>
    <group ns="left_os1">
        <include file="$(find ouster_ros)/os1.launch" >
            <arg name="os1_hostname" value="192.168.1.204"/>
            <arg name="os1_udp_dest" value="192.168.1.54"/>
            <arg name="os1_lidar_port" default="7502" doc="port to which the sensor should send lidar data"/>
            <arg name="os1_imu_port" default="7503" doc="port to which the sensor should send imu data"/>
            <arg name="tf_prefix" default="left_os1"/>
        </include>
    </group>
</launch>