<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--
    Nissan Bringup verison 2020.A:
    - LIDAR: 2 x Ouster OS1-64
    - LIDAR: 2 x Velodyne VLP-16
    - LIDAR: 1 x Sick
    - GPS:   1 x Duro 
    - GPS:   1 x NovAtel (map->base_link nova_global_frame_tf_publisher.launch)
    - CAM:   1 x Zed
    - CAN:   can_driver.gen.launch
    - TF:    nissanleaf_statictf_launch.launch
-->
<launch>
  <include file="$(find nissan_bringup)/launch/sensory/ouster_two_lidar.launch" />
  <include file="$(find nissan_bringup)/launch/sensory/velodyne_two_lidar.launch" />
  <include file="$(find nissan_bringup)/launch/sensory/sick_lms_111.nissan.launch" />
  <include file="$(find nmea_navsat_driver)/launch/nmea_serial_driver.launch" />
  <include file="$(find nissan_bringup)/launch/sensory/gps.duro.launch" />
  <include file="$(find nissan_bringup)/launch/sensory/gps.nova.launch" />
  <!--  <include file="$(find nissan_bringup)/launch/tf_setup/tf_duro_global_frame_tf_publisher.launch" />-->
  <include file="$(find zed_wrapper)/launch/zed_no_tf.launch" />
  <!--  <include file="$(find nissan_bringup)/launch/low_level/can_driver.gen.launch" />-->
  <!--<include file="$(find nissan_bringup)/launch/tf_setup/tf_novatel_global_frame_tf_publisher.statictf.launch" /> -->
  <!-- <include file="$(find nissan_bringup)/launch/tf_setup/tf_novatel_global_frame_tf_publisher.launch" /> -->
  <include file="$(find nissan_bringup)/launch/tf_setup/tf_nissanleaf_statictf.launch" />
 <include file="$(find nissan_bringup)/launch/tf_setup/tf_duro_global_frame_tf_publisher.statictf.launch" />
  <include file="$(find leaf_can_driver)/launch/nissan_can_control.launch" />
  <node pkg="nissan_bringup" type="vehicle_cmd_converter.py" name="vehicle_cmd_converter"/> <!-- temporary fix TODO-->
  <node pkg="nissan_bringup" type="current_pose_from_tf.py" name="current_pose_from_tf"/> <!-- temporary fix TODO-->
  <node pkg="topic_tools" type="relay" name="cmd_relay" args="/ctrl_raw /ctrl_cmd" />
  
</launch>
